{
    "name": "sancherie/feature",
    "description": "This is my package feature",
    "keywords": [
        "sancherie",
        "laravel",
        "feature"
    ],
    "homepage": "https://github.com/sancherie/feature",
    "license": "MIT",
    "authors": [
        {
            "name": "Cl√©ment SANCHEZ",
            "email": "clement@sanchez.sh",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^1.0.3",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Sancherie\\Feature\\": "src",
            "Sancherie\\Feature\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sancherie\\Feature\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sancherie\\Feature\\FeatureServiceProvider"
            ],
            "aliases": {
                "Feature": "Features"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
